{
  "name": "electron-ipc-sample",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:mac": "electron-packager . --asar --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds --darwinDarkModeSupport=true",
    "package:win": "electron-packager . --asar --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds"
  },
  "author": "hrdtbs",
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-packager": "^15.2.0"
  }
}
